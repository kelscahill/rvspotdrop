{% if primary_nav.items %}
  <nav class="c-nav-primary" role="navigation">
    <ul class="c-nav-primary__list">
      {% for item in primary_nav.items %}
        <li class="{{ item.classes|join(' c-nav-primary__') }}">
          <a class="c-nav-primary__link" href="{{ item.link }}"{{ item.target ? ' target="' ~ item.target ~'"'}}>{{ item.title }}</a>
        </li>
      {% endfor %}
    </ul>
  </nav>
{% endif %}